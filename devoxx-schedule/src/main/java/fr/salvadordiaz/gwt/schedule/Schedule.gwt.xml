<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='schedule'>
	<inherits name="com.google.common.collect.Collect"/>
	<inherits name="com.google.gwt.inject.Inject"/>
	<inherits name='com.google.gwt.user.User' />
	<inherits name="com.google.gwt.logging.Logging" />
	<inherits name="com.google.gwt.activity.Activity" />
	<inherits name="com.google.gwt.place.Place" />
	<inherits name="com.google.gwt.json.JSON" />
	<inherits name="com.google.gwt.jsonp.Jsonp" />
	<entry-point class='fr.salvadordiaz.gwt.schedule.client.Schedule' />

	<source path='client' />
	<source path='shared' />

	<!-- no ie6, ie7 nor old gecko support -->
	<set-property name="user.agent" value="ie8,gecko1_8,safari,opera"/>
	
	<set-property name="gwt.logging.popupHandler" value="DISABLED"/>
</module>
